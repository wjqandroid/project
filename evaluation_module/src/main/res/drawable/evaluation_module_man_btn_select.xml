<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval"
            android:useLevel="false">
            <solid android:color="@color/COLOR_BLUE_75C4EF" />
            <stroke android:color="@color/COLOR_BLUE_75C4EF" />
            <size
                android:width="40dp"
                android:height="40dp" />
        </shape>
    </item>
    <item
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp">
        <bitmap android:src="@drawable/evaluation_module_man_select" />

    </item>
</layer-list>