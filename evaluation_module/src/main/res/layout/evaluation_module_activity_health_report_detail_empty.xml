<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/base_module_common_toolbar" />

    <androidx.viewpager.widget.ViewPager
        android:id="@+id/evaluation_module_health_report_detail_empty_viewpager"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:padding="16dp"
        android:layout_weight="1"/>

    <LinearLayout
        android:id="@+id/evaluation_module_health_report_detail_circle_layout"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginHorizontal="25dp"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/indicator_select"
            android:layout_width="12dp"
            android:layout_height="5dp"
            android:src="@drawable/evaluation_module_health_report_detail_indicator_select"
            />

        <ImageView
            android:id="@+id/indicator_unselect"
            android:layout_width="12dp"
            android:layout_height="5dp"
            android:layout_marginLeft="10dp"
            android:src="@drawable/evaluation_module_health_report_detail_indicator_unselect"
            />

    </LinearLayout>

</LinearLayout>
